{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@validators": ["src/validators.js"],
      "@scss/*": ["src/scss/*"],
      "@test": ["src/test/index.js"],
      "@test/*": ["src/test/*"],
      "@/js/utils": ["src/js/utils/index.js"],
      "@/vuex": ["src/vuex/index.js"],
      "@/*": ["src/*"],
      "@static/*": ["static/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
